{
  "name": "fireswag-backend",
  "version": "1.0.0",
  "description": "Fireswag online store.",
  "main": "dist/server.js",
  "scripts": {
    "watch": "nodemon",
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "build": "rm -rf dist && tsc && cp config.yml ./dist",
    "dev": "ts-node ./src/server.ts",
    "prod": "npm run build && node dist/server.js",
    "dist": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "swagger:generate": "npx swagger-typescript-api --no-client -p ./swagger.yml -o ./src/models/swagger -n index.ts"
  },
  "keywords": [
    "fireswag"
  ],
  "author": "Lokesh Babu <lokeshbabu.gp95@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.20.0",
    "base64-stream": "^1.0.0",
    "body-parser": "^1.18.3",
    "bwip-js": "^2.1.3",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "fabric-ca-client": "~1.4.0",
    "fabric-network": "~1.4.0",
    "fast-csv": "^4.3.1",
    "hl7-standard": "^1.0.2",
    "http-proxy-middleware": "^1.0.5",
    "ics": "^2.29.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-geocoder": "^3.27.0",
    "nodemailer": "^6.4.11",
    "number-to-words": "^1.2.4",
    "openpgp": "^5.0.1",
    "pdfkit": "^0.11.0",
    "prom-client": "^13.1.0",
    "prometheus-api-metrics": "^3.2.0",
    "puppeteer": "^8.0.0",
    "qrcode": "^1.4.4",
    "redis": "^2.8.0",
    "request-ip": "^2.1.3",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.28.3",
    "socket.io": "^2.3.0",
    "sparkpost": "^2.1.4",
    "ssh2": "^0.8.9",
    "swagger-typescript-api": "^1.12.0",
    "swagger-ui-express": "^4.1.4",
    "twilio": "^3.48.2",
    "ua-parser-js": "^0.7.21",
    "uuid": "^8.2.0",
    "winston": "^3.2.1",
    "xlsx": "^0.16.9",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongoose": "^5.5.34",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.12.17",
    "@types/node-geocoder": "^3.24.1",
    "@types/pdfkit": "^0.11.0",
    "@types/redis": "^2.8.14",
    "@types/sharp": "^0.28.3",
    "@types/socket.io": "^2.1.4",
    "@types/sparkpost": "^2.1.4",
    "@types/ssh2": "^0.5.45",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.19.1",
    "nodemon": "^2.0.7",
    "ts-node": "^8.5.4",
    "typescript": "^4.2.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      ".env",
      "swagger.yml",
      "config.yml"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
